    "require": {
        "php": ">=5.4.0 || >=5.5.9",
        "ext-soap": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "composer/composer": "^1.0",
        "symfony/console": "^2.8 || ^3.0 || ^4.0",
        "symfony/yaml": "^2.8 || ^3.0 || ^4.0",
        "wsdltophp/domhandler": "^1.0",
        "wsdltophp/packagebase": "~1.0",
        "wsdltophp/phpgenerator": "^1.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "lint": "vendor/bin/php-cs-fixer fix --ansi --diff --verbose",
        "build": "box build --verbose"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^4.8 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "WsdlToPhp\\PackageGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WsdlToPhp\\PackageGenerator\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
